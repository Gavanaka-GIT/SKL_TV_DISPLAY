{
  "name": "tv-display",
  "bin": "src/server.js",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon src/server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "tv-display": "concurrently --kill-others-on-fail \"yarn server\"",
    "build": "nexe src/server.js"
  },
  "pkg": {
    "scripts": [],
    "assets": [
      "src/build/images/*",
      "src/build/**/*",
      "node_modules/**/*"
    ],
    "targets": [
      "node16-win-x64"
    ]
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "hashmap": "^2.4.0",
    "json": "^11.0.0",
    "moment": "^2.29.3",
    "oracledb": "^5.2.0",
    "pkg": "^5.8.1"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nexe": "^4.0.0-rc.2"
  }
}
